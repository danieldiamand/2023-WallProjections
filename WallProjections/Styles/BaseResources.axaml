<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:WallProjections.Styles.Converters">

    <Thickness x:Key="ControlPadding">4</Thickness>
    <Thickness x:Key="OverlayPadding">8</Thickness>
    <CornerRadius x:Key="ControlCornerRadius">4</CornerRadius>
    <CornerRadius x:Key="OverlayCornerRadius">8</CornerRadius>

    <!-- Converters -->
    <converters:ThicknessConverter x:Key="ThicknessConverter" />
    <converters:CornerRadiusConverter x:Key="CornerRadiusConverter" />
    <converters:ThicknessMultiConverter x:Key="ThicknessMultiConverter" />
    <converters:CornerRadiusMultiConverter x:Key="CornerRadiusMultiConverter" />
    <converters:SpacingConverter x:Key="SpacingConverter" />

</ResourceDictionary>
